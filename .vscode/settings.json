{
    "editor.formatOnSave": true,
    "[dart]": {
        "editor.defaultFormatter": "Dart-Code.dart-code",
        "editor.formatOnSave": true,
        "editor.tabSize": 2
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": "always",
        "source.organizeImports": "always"
    },
    "dart.analysisExcludedFolders": [
        "build",
        ".dart_tool"
    ],
    "dart.lineLength": 80,
    "dart.singleQuotes": true,
    "dart.showTodos": true
}